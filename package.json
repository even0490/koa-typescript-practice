{
  "name": "koa-typescript-practice",
  "version": "1.0.0",
  "description": "A koa and typescript practice and want to look for a girlfriend...",
  "main": "server/index.js",
  "engines": {
    "node": ">=8.11.2"
  },
  "scripts": {
    "start": "NODE_ENV=local nodemon --watch 'server/**/*' -e ts,tsx --exec 'ts-node' ./server/index.ts",
    "development": "NODE_ENV=development nodemon build/index.js",
    "production": "NODE_ENV=production NODE_DEV=localPro nodemon build/index.js",
    "watch": "node ./webpack/watch.js",
    "build": "node ./webpack/build.js",
    "watch-ts": "tsc -w",
    "build-ts": "rm -rf ./build/* && tsc"
  },
  "keywords": [
    "koa",
    "typescript",
    "practice"
  ],
  "author": "meizikeai@163.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/meizikeai/koa-typescript-practice.git"
  },
  "dependencies": {
    "handlebars": "^4.1.2",
    "koa": "^2.7.0",
    "koa-base-logger": "0.0.7",
    "koa-better-cors": "0.0.2",
    "koa-body": "^4.1.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.2.0",
    "koa-json": "^2.0.2",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.4.0",
    "koa-safe-jsonp": "^0.3.1",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "node-fetch": "^2.6.0",
    "node-qconf": "^0.3.1",
    "raven": "^2.6.4",
    "redis": "^2.8.0",
    "redis-commands": "^1.5.0"
  },
  "devDependencies": {
    "@antv/f2": "^3.3.9",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.3",
    "@types/mkdirp": "^0.5.2",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.0.10",
    "@types/raven": "^2.5.3",
    "@types/redis": "^2.8.13",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-blued": "^1.1.0",
    "eslint-config-blued-react": "^1.1.0",
    "eslint-config-blued-typescript": "^1.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.4.1",
    "lodash.debounce": "^4.0.8",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^3.4.0",
    "postcss-loader": "^3.0.0",
    "qiniu-webpack-plugin": "^0.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-lazyload": "^2.6.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "browserslist": [
    "> 2%",
    "last 5 versions",
    "Android >= 4"
  ],
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "logs/*",
      "node_modules/*",
      "public/*"
    ],
    "events": {
      "restart": "osascript -e 'display notification \"koa-typescript-practice restarted\" with title \"nodemon\"'"
    },
    "watch": [
      "server/*"
    ],
    "ext": "ts js json jsx scss hbs html ejs css sass"
  },
  "eslintIgnore": [
    "build/**/*"
  ]
}
